<script>

function handleNewElements(elements) {
  elements.forEach(element => {
      var imgbox = element.querySelector('img');
      if (imgbox) {
        console.log(imgbox)
          imgbox.setAttribute('width', '400');
          imgbox.setAttribute('height', '400');
      }
  });
}

const existingElements = document.querySelectorAll('.bx-row-image');
if (existingElements.length) {
  handleNewElements(existingElements);
}

const observer = new MutationObserver((mutationsList) => {
  for (let mutation of mutationsList) {
      if (mutation.type === 'childList' && mutation.addedNodes.length > 0) {
          mutation.addedNodes.forEach(node => {
              if (node.nodeType === Node.ELEMENT_NODE) {
                  if (node.classList.contains('bx-row-image')) {
                      handleNewElements([node]);
                  }
                  
                  const nestedElements = node.querySelectorAll('.bx-row-image');
                  if (nestedElements.length) {
                      handleNewElements(nestedElements);
                  }
              }
          });
      }
  }
});

observer.observe(document.body, { childList: true, subtree: true });


  </script>
